	GUI.NonInv.txtRev.Text("")
	GUI.NonInv.txtDescr.Text("")
	GUI.NonInv.txtDescr2.Text("")
	GUI.NonInv.txtDescr3.Text("")
	GUI.NonInv.txtUM.Text("")
	GUI.NonInv.txtPL.Text("")
	GUI.NonInv.txtExtra.Text("")
