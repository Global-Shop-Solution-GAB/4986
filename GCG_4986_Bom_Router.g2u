	f.Intrinsic.String.Build("SELECT Sum(EST_UM_UNITS) FROM RTR_CutList where Part = '{0}' and Component = '{1}'",v.Local.sPart.PSQLFriendly,v.Local.sComponent.PSQLFriendly,v.Local.sSQL)
