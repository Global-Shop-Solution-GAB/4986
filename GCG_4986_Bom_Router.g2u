		f.Intrinsic.Control.CallSub(Deal_OnTop,"Flag",True)
		gui.NonInv.ddlSource.SetFocus
		f.Intrinsic.Control.ExitSub
	f.Intrinsic.Control.EndIf
	'Validate UM
	f.Intrinsic.Control.If(v.Screen.NonInv!txtUM.text.Trim,=,"")
