	f.Intrinsic.Control.EndIf
	
	f.Intrinsic.Control.If(v.DataTable.BUILDQTY.RowCount,=,0)

		f.Intrinsic.Control.If(v.Screen.Form!txtFocusParent.text.Trim,<>,"")
