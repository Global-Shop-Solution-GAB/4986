			F.Intrinsic.Control.Case("H")
				V.Global.sLaborType.Set("HPP")
			F.Intrinsic.Control.Case("M")
				V.Global.sLaborType.Set("MPP")
		f.Intrinsic.Control.EndSelect
		v.Global.sType.Set("HPP")
