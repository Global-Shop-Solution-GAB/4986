			'fill in extra text
			f.Intrinsic.Control.If(v.Local.sText.IsNullOrWhiteSpace,=,False)
				f.Intrinsic.String.Build("select part, text from inv_extra_text where part = '{0}'",V.Local.sPart.PSQLFriendly,v.Local.sSql)
