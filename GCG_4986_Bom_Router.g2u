	f.Data.DataTable.Compute("CHILD","Max(Serie)","",v.Local.iSerie)
	f.Intrinsic.Math.Add(v.Local.iSerie,1,v.Local.iSerie)		
	
	'Add 99 record to the current Router
	v.Local.fCost.Set(v.Screen.NonInv!txtCost.Text.Trim)
