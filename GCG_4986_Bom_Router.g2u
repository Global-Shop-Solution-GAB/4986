	f.Data.DataView.Close("CHILD","vNext99")
	
	'Source Code and Category
	f.Data.Dictionary.ReturnKeyFromValue("SOURCE",V.Screen.NonInv!ddlSource.TEXT,False,v.local.sSource_Code)	
