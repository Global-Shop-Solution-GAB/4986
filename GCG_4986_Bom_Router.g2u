	v.Local.sSQL.Declare

	f.Data.DataView.Create("Bom","vLoc",22,"","")
	f.Data.DataView.ToString("Bom","vLoc","Part","","#@$",v.Local.sPartList)
	f.Intrinsic.String.Replace(v.Local.sPartList,"'","''",v.Local.sPartList)
	f.Intrinsic.String.Replace(v.Local.sPartList,"#@$","','",v.Local.sPartList)
